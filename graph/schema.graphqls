type User{
  _id: String!
  name: String!
  email: String!
  address: String!
  password: String!
  phone: String!
}

type Query{
  user(_id: String!): User!
  users: [User!]!
}


input newUser{
  name: String!
  email: String!
  address: String!
  password: String!
  phone: String!
}

type Mutation{
  createUser(input: newUser): User!
}